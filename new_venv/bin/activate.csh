# Unset irrelevant variables.
deactivate nondestructive

setenv VIRTUAL_ENV "/Users/tiga/Documents/Uni/Internships and Jobs/PhD/Rotation 1/Software Group Project/PK_the_other_side_of_the_story/new_venv"

set _OLD_VIRTUAL_PATH="$PATH"
setenv PATH "$VIRTUAL_ENV/bin:$PATH"


